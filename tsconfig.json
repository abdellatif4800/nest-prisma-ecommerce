{
  "compilerOptions": {
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "resolvePackageJsonExports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "apiLibs/auth": [
        "libs/auth/src"
      ],
      "apiLibs/auth/*": [
        "libs/auth/src/*"
      ],
      "apiLibs/cart-management": [
        "libs/cart-management/src"
      ],
      "apiLibs/cart-management/*": [
        "libs/cart-management/src/*"
      ],
      "apiLibs/categories": [
        "libs/categories/src"
      ],
      "apiLibs/categories/*": [
        "libs/categories/src/*"
      ],
      "apiLibs/common": [
        "libs/common/src"
      ],
      "apiLibs/common/*": [
        "libs/common/src/*"
      ],
      "apiLibs/file-storage": [
        "libs/file-storage/src"
      ],
      "apiLibs/file-storage/*": [
        "libs/file-storage/src/*"
      ],
      "apiLibs/order-management": [
        "libs/order-management/src"
      ],
      "apiLibs/order-management/*": [
        "libs/order-management/src/*"
      ],
      "apiLibs/prisma-setup": [
        "libs/prisma-setup/src"
      ],
      "apiLibs/prisma-setup/*": [
        "libs/prisma-setup/src/*"
      ],
      "apiLibs/products": [
        "libs/products/src"
      ],
      "apiLibs/products/*": [
        "libs/products/src/*"
      ],
      "src/*": [
        "src/*"
      ]
    }
  }
}