{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^api/test-lib1(|/.*)$": "<rootDir>/../libs/test-lib1/src/$1",
    "^apiLibs/prisma-setup(|/.*)$": "<rootDir>/../libs/prisma-setup/src/$1",
    "^src/(.*)$": "<rootDir>/../src/$1"
  }
}